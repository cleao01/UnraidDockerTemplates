<?xml version="1.0"?>
<Container version="2">
  <Name>ApacheGuacamoleNoMariaDB</Name>
  <Repository>cleao/guacamole:latest-nomariadb</Repository>
  <Registry>https://registry.hub.docker.com/r/cleao/guacamole</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/54855-support-jasonbean-apache-guacamole/</Support>
  <Project>https://github.com/cleao01/docker-guacamole</Project>
  <Overview>Apache Guacamole is a clientless remote desktop gateway. It supports standard protocols like VNC and RDP.&#xD;
    Note: This version comes without MariaDB and requires some external database source or to use the default authentication.&#xD;
    /config : this path is where Apache Guacamole will store it's properties files and extensions.&#xD;
    Initial username and password are guacadmin</Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:8086]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/cleao01/docker-guacamole/blob/0aa2a7522005f4d1b81e639b334cdc8c577b6959/GuacaIcon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1756218044</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Web Port" Target="8080" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">8086</Config>
  <Config Name="OPT_MYSQL" Target="OPT_MYSQL" Default="Y" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">Y</Config>
  <Config Name="OPT_SQLSERVER" Target="OPT_SQLSERVER" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_LDAP" Target="OPT_LDAP" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_DUO" Target="OPT_DUO" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_CAS" Target="OPT_CAS" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_TOTP" Target="OPT_TOTP" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_QUICKCONNECT" Target="OPT_QUICKCONNECT" Default="N" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_HEADER" Target="OPT_HEADER" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name=" OPT_SAML" Target=" OPT_SAML" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/ApacheGuacamoleNoMariaDB" Mode="rw" Description="" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/ApacheGuacamoleNoMariaDB</Config>
  <TailscaleStateDir/>
</Container>