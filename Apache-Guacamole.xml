<?xml version="1.0"?>
<Container version="2">
  <Name>Apache-Guacamole</Name>
  <Repository>cleao/guacamole:latest</Repository>
  <Registry>https://registry.hub.docker.com/r/cleao/guacamole/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/54855-support-jasonbean-apache-guacamole/</Support>
  <Project>https://guacamole.apache.org/</Project>
  <Overview>Apache GuacamoleApache Guacamole is a clientless remote desktop gateway. It supports standard protocols like VNC and RDP.&#xD;
&#xD;
&#xD;
    Note: This version comes with MariaDB and use the default authentication.&#xD;
&#xD;
    Directions:&#xD;
&#xD;
    Be sure to change the Host port from 8080 to something else as 8080 is quite a common port.&#xD;
&#xD;
&#xD;
    /config : this path is where Apache Guacamole will store it's properties files, extensions, and the database which contains data for users and connections.&#xD;
&#xD;
    Initial username and password are guacadmin</Overview>
  <Category>Productivity: Network:Other Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/jason-bean/docker-templates/master/jasonbean-repo/Guacamole-nomariadb.xml</TemplateURL>
  <Icon>https://github.com/cleao01/docker-guacamole/blob/0aa2a7522005f4d1b81e639b334cdc8c577b6959/GuacaIcon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1756206648</DateInstalled>
  <DonateText>Donate To Author</DonateText>
  <DonateLink>https://paypal.me/jasonsbean/0usd</DonateLink>
  <Requires/>
  <Config Name="Web Port" Target="8080" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">8086</Config>
  <Config Name="OPT_MYSQL" Target="OPT_MYSQL" Default="Y" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">Y</Config>
  <Config Name="OPT_SQLSERVER" Target="OPT_SQLSERVER" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_LDAP" Target="OPT_LDAP" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_DUO" Target="OPT_DUO" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_CAS" Target="OPT_CAS" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_TOTP" Target="OPT_TOTP" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_QUICKCONNECT" Target="OPT_QUICKCONNECT" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_HEADER" Target="OPT_HEADER" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="OPT_SAML" Target="OPT_SAML" Default="N" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">N</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/ApacheGuacamoleNoMariaDB-1" Mode="rw" Description="" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/Apache-GuacamoleNoMariaDB</Config>
  <Config Name="PUID" Target="PUID" Default="90" Mode="" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">100</Config>
  <TailscaleStateDir/>
</Container>